algoritmo "TesteVetor"
// Função :Verores
// Autor :MFF
// Data : 09/01/2021
// Seção de Declarações 
var
    i:inteiro
    n: vetor[1..4] de inteiro

inicio
     Para i <- 1 ate 4 faca
      Escreva("Digite um numero:")
      leia(n[i])
     FimPara
      Para i <- 1 ate 4 faca
       Escreva("{", n[i], "}")
      FimPara
fimalgoritmo
