algoritmo "ALGa0001"
// Função :
// Autor :
// Data : 10/01/2021
// Seção de Declarações 
var
    RES,CONT,X,N:INTEIRO
inicio
      CONT <- N
      RES <- 0
      X <- 2
      N <- 4
      ENQUANTO CONT >= N FACA
          RESP <- RESP * X
          CONT <- CONT + 1
      FIMENQUANTO
      ESCREVA(RES)
fimalgoritmo
